                        </desc_content>
                    </desc>
                    <index entries="['single',\ 'name\ (horizon.PanelGroup\ attribute)',\ 'horizon.PanelGroup.name',\ '',\ None]"></index>
                    <desc desctype="attribute" domain="py" noindex="False" objtype="attribute">
                        <desc_signature class="PanelGroup" first="False" fullname="PanelGroup.name" ids="horizon.PanelGroup.name" module="horizon" names="horizon.PanelGroup.name"><desc_name xml:space="preserve">name</desc_name></desc_signature>
