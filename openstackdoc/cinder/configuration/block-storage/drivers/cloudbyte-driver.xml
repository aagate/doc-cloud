                        <row>
                            <entry>
                                <paragraph><literal>cb_add_qosgroup</literal> = <literal>{'latency': '15', 'iops': '10', 'graceallowed': 'false', 'iopscontrol': 'true', 'memlimit': '0', 'throughput': '0', 'tpcontrol': 'false', 'networkspeed': '0'}</literal></paragraph>
