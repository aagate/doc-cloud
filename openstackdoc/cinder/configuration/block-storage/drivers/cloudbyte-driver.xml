                        <row>
                            <entry>
                                <paragraph><literal>cb_create_volume</literal> = <literal>{'compression': 'off', 'deduplication': 'off', 'blocklength': '512B', 'sync': 'always', 'protocoltype': 'ISCSI', 'recordsize': '16k'}</literal></paragraph>
